class Link {
    label: string;
    url: string;
    subCategory?: string;

    constructor(label:string, url:string, subCategory?:string) {
        this.label = label;
        this.url = url;
        this.subCategory = subCategory ? subCategory : 'main';
    }
}

// returns an array of site links 
function getSiteLinks (){
    let siteLinks = [Link];
    return siteLinks;
}

export { Link, getSiteLinks };